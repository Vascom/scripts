#!/bin/bash

SEQ="4 5 6"
NUMBER_LINES=`cat $1 | head -n 1 | cut -d "," -f1`
TEMP_LINE=`cat $1 | head -n $NUMBER_LINES | tail -n +2`

for n in $TEMP_LINE
do
    for k in $SEQ
    do
        STATE=`echo $n | cut -d "," -f$k`
        if [ "$STATE" = "-" ]
        then
            NAME=`echo $n | cut -d "," -f1`
            REV=`echo $n | cut -d "," -f3`
            BUILD=`expr $k + 10`
            ./koji_run $NAME $REV $BUILD quiet
        fi
    done
done
